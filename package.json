{
  "name": "@aureooms/js-array",
  "description": "array manipulation code bricks for JavaScript",
  "version": "4.0.0",
  "author": "Aur√©lien Ooms <aurelien.ooms@gmail.com>",
  "ava": {
    "require": [
      "regenerator-runtime/runtime",
      "@babel/register"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "test": {
        "presets": [
          "babel-preset-power-assert"
        ],
        "plugins": [
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ],
        "sourceMaps": "inline"
      },
      "development": {
        "presets": [
          "babel-preset-power-assert"
        ],
        "plugins": [
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ],
        "sourceMaps": "inline"
      },
      "production": {
        "plugins": [
          "babel-plugin-unassert",
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ],
        "sourceMaps": "inline"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/aureooms/js-array/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "@aureooms/js-array": "^4.0.0",
    "@aureooms/js-compare": "^1.4.5",
    "@aureooms/js-functools": "^2.0.3",
    "@aureooms/js-itertools": "^3.3.0",
    "@aureooms/js-memory": "^4.0.0",
    "@aureooms/js-operator": "^1.0.2",
    "@aureooms/js-random": "^2.0.0",
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.1",
    "codeclimate-test-reporter": "^0.5.0",
    "coveralls": "^2.13.1",
    "esdoc": "^1.1.0",
    "nyc": "^11.0.1",
    "@babel/cli": "7.12.10",
    "@babel/core": "7.12.10",
    "@babel/preset-env": "7.12.10",
    "@babel/register": "7.12.10",
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-plugin-unassert": "3.0.1",
    "babel-preset-power-assert": "3.0.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "np": "6.5.0",
    "power-assert": "1.6.1"
  },
  "homepage": "http://aureooms.github.io/js-array/",
  "keywords": [
    "array",
    "bricks",
    "javascript",
    "js",
    "loop",
    "vector"
  ],
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/aureooms/js-array.git"
  },
  "scripts": {
    "build": "babel src -d lib",
    "cover": "nyc --reporter=lcov npm test",
    "prepublish": "npm run build",
    "test": "ava ./test/src --concurrency 8",
    "esdoc": "esdoc"
  }
}
